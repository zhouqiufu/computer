<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.maintain.domain">

	<!-- OR mapping -->
	<class name="Maintain" table="Maintain">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="name" type="string" column="name"
			length="60" />
		 <property name="phone" type="string" column="phone"
			length="60" />
		 <property name="address" type="string" column="address"
			length="60" />
		 <property name="freetime" type="string" column="freetime"
			length="60" />
		 <property name="weihu" type="string" column="weihu"
			length="60" />
		 <property name="shebei" type="string" column="shebei"
			length="60" />
		 <property name="pingtai" type="string" column="pingtai"
			length="60" />
		 <property name="guzhang" type="string" column="guzhang"
			length="60" />
		 <property name="content" type="string" column="content"
			length="1000" />
		<property name="dateTime" type="string" column="dateTime"
			length="60" />
	</class>

</hibernate-mapping>