<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.AnnounceArea.domain">

	<!-- OR mapping -->
	<class name="AnnounceDomain" table="AnnounceDomain">
		<id name="announceId" type="string" column="announceId">
			<generator class="assigned" />
		</id>
		<property name="announceTitle" type="string" column="announceTitle"
			length="200" />
		<property name="announceContent" type="string" column="announceContent"
			length="10000" />
		<property name="fullTime" type="string" column="fullTime"
			length="50" />
		<property name="newsDate" type="string" column="newsDate"
			length="50" />
		<property name="announceAuthor" type="string" column="announceAuthor"
			length="50" />		
		<property name="html_path" type="string" column="html_path"
			length="100" />
		<property name="photo_path" type="string" column="photo_path"
			length="100" />
		<property name="photo_explain" type="string" column="photo_explain"
			length="100" />

		<property name="photoName" type="string" column="photoName"
			length="100" />

		<property name="htmlName" type="string" column="htmlName"
			length="100" />

	</class>

</hibernate-mapping>