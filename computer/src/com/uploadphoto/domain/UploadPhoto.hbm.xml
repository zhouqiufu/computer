<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.uploadphoto.domain">

	<!-- OR mapping -->
	<class name="UploadPhoto" table="UploadPhoto">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="photoName" type="string" column="photoName"
			length="100" />
		<property name="photoPath" type="string" column="photoPath"
			length="150" />
		<property name="fullTime" type="string" column="fullTime"
			length="100" />
		<property name="photoExplain" type="string" column="photoExplain"
			length="100" />
			
	</class>

</hibernate-mapping>