<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
 	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<!-- 开始 -->
    
	<package name="actionPackage" extends="struts-default">

		<action name="pagingAction" class="com.publicAction.pagingAction">
			<!-- result 标签中的name属性的值都是小写的单词 -->
			<result name="DN">/public/paging_600.jsp</result>
			<result name="Announce">/public/paging_610.jsp</result>
			<result name="Technology">/public/paging_620.jsp</result>
			
			<result name="error">/error/ReturnHomePage.jsp</result>
		</action>


		<action name="loginInfoAction" class="com.login.action.LoginInfoAction">
			<result name="error">/error/ReturnHomePage.jsp</result>
		</action>
		
		
	
		

	</package>


</struts>
